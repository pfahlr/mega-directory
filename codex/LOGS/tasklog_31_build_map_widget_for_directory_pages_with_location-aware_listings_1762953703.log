+
+export function buildDirectoryMapPins(directory, listingsOverride) {
+  const listings = Array.isArray(listingsOverride)
+    ? listingsOverride
+    : Array.isArray(directory?.listings)
+      ? directory.listings
+      : [];
+
+  if (!listings.length) {
+    return [];
