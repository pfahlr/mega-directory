+          </button>
+          <button
+            type="button"
+            class="map-action-button map-action-button--accent"
+            data-map-action="nav"
+            data-map-target={mapId}
+          >
+            Open in maps
+          </button>
+        </div>
+      </div>
+    </section>
+    <script type="module" is:inline>
+      const mapId = {JSON.stringify(mapId)};
