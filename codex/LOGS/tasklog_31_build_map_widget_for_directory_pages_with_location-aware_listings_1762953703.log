+        <p class="text-xs text-slate-400">
+          {description}
+        </p>
+        <div class="flex flex-wrap gap-3" data-map-actions>
+          <button
+            type="button"
+            class="map-action-button"
+            data-map-action="csv"
+          >
+            Export CSV
+          </button>
+          <button
+            type="button"
+            class="map-action-button"
+            data-map-action="gpx"
+          >
+            Export GPX
+          </button>
+          <button
+            type="button"
+            class="map-action-button map-action-button--accent"
