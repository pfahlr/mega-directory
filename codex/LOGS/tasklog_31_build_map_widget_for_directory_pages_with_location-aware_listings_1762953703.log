+      </div>
+    </section>
+    <script type="module" is:inline>
+      const mapId = {JSON.stringify(mapId)};
+      const mapElement = document.getElementById(mapId);
+      if (!mapElement || mapElement.dataset.initialized) {
+        return;
+      }
+
+      mapElement.dataset.initialized = 'true';
+      const pins = (() => {
