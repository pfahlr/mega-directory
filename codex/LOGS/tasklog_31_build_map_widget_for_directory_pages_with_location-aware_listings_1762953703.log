+      let activeSlug = initialSlug;
+
+      const findPin = (slug) => pins.find((pin) => pin.slug === slug);
+      const updateActiveDetails = (pin) => {
+        if (!pin) {
+          return;
+        }
+        if (activeNameEl) {
