+        }
+      };
+
+      const applyFeatureStyles = () => {
+        vectorLayer.getSource().getFeatures().forEach((feature) => {
+          feature.setStyle(feature.getId() === activeSlug ? activeStyle : defaultStyle);
+        });
+      };
+
+      const focusListing = (slug) => {
+        const pin = findPin(slug);
