+      const exportGpx = () => {
+        const wpts = pins
+          .map(
+            (pin) =>
+              `  <wpt lat="${pin.latitude}" lon="${pin.longitude}">\n    <name>${escapeXml(pin.name)}</name>\n    <desc>${escapeXml(pin.summary ?? pin.locationLabel ?? '')}</desc>\n  </wpt>`
