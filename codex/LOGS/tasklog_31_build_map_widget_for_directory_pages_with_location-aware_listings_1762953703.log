+        if (activeNameEl) {
+          activeNameEl.textContent = pin.name;
+        }
+        if (activeLocationEl) {
+          activeLocationEl.textContent = pin.locationLabel || `${pin.latitude}, ${pin.longitude}`;
+        }
+      };
+
+      const applyFeatureStyles = () => {
+        vectorLayer.getSource().getFeatures().forEach((feature) => {
