+        } else if (action === 'gpx') {
+          button.addEventListener('click', exportGpx);
+        } else if (action === 'nav') {
+          button.addEventListener('click', openNavigation);
+        }
+      });
+
+      focusListing(activeSlug);
+    </script>
+  </>
+)}
