+          }
+        });
+      });
+
+      focusButtons.forEach((button) => {
+        button.addEventListener('click', () => {
+          const slug = button.getAttribute('data-map-slug');
+          if (slug) {
+            focusListing(slug);
+          }
+        });
+      });
+
+      const toCsvValue = (value) => {
+        if (value === null || value === undefined) {
+          return '';
+        }
+        const stringValue = String(value);
