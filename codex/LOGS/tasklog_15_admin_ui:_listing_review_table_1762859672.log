+  listingsStore.splice(0, listingsStore.length, ...JSON.parse(JSON.stringify(originalListings)));
+}
+
+test('getListings paginates records and reports metadata', () => {
+  resetListings();
+
+  const perPage = 2;
+  const result = getListings({ page: 1, perPage });
+
