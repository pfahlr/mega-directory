+:root {
+  color-scheme: dark;
+  font-family: "Inter", "Inter var", -apple-system, BlinkMacSystemFont, "Segoe UI",
+    system-ui, sans-serif;
+  --color-bg: #010914;
+  --color-bg-muted: #020f21;
+  --color-text: #e2e8f0;
+  --color-accent: #34d399;
+  --color-border: rgba(148, 163, 184, 0.2);
+  --radius-lg: 1.5rem;
+  text-rendering: optimizeLegibility;
+}
+
+*,
+*::before,
+*::after {
+  box-sizing: border-box;
+}
+
+body {
+  margin: 0;
+  min-height: 100vh;
+  font-family: inherit;
+  background: radial-gradient(circle at top, rgba(79, 70, 229, 0.3), transparent 45%),
+    radial-gradient(circle at 20% 20%, rgba(16, 185, 129, 0.25), transparent 40%),
+    var(--color-bg);
+  color: var(--color-text);
+  line-height: 1.6;
+}
+
+img,
+svg {
+  max-width: 100%;
+  display: block;
+}
+
+a:not([class]) {
+  color: inherit;
+  text-decoration: underline;
+  text-decoration-thickness: 2px;
+  text-underline-offset: 0.2em;
+}
+
+button,
+input,
+select,
+textarea {
+  font: inherit;
+}
+
+@media (prefers-reduced-motion: reduce) {
+  *,
+  *::before,
+  *::after {
+    animation-duration: 0.01ms !important;
+    animation-iteration-count: 1 !important;
+    transition-duration: 0.01ms !important;
+    scroll-behavior: auto !important;
+  }
+}
+
+.skip-link {
+  position: absolute;
+  z-index: 50;
+  margin: 0.5rem;
+  padding: 0.75rem 1.25rem;
+  background: #0f172a;
+  color: #fafafa;
+  border-radius: 999px;
+  transform: translateY(-200%);
+  transition: transform 0.2s ease;
+}
+
+.skip-link:focus-visible {
+  transform: translateY(0);
+  outline: 2px solid var(--color-accent);
+  outline-offset: 4px;
+}
+
+.sr-only {
+  position: absolute;
+  width: 1px;
+  height: 1px;
+  padding: 0;
+  margin: -1px;
+  overflow: hidden;
+  clip: rect(0, 0, 0, 0);
+  white-space: nowrap;
+  border: 0;
+}
+
+.card-surface {
+  background: linear-gradient(135deg, rgba(15, 23, 42, 0.85), rgba(2, 6, 23, 0.95));
+  border: 1px solid var(--color-border);
+  border-radius: var(--radius-lg);
+  box-shadow: 0 20px 80px rgba(15, 23, 42, 0.45);
+}
+
+.glass-panel {
+  background: rgba(2, 6, 23, 0.65);
+  border: 1px solid rgba(148, 163, 184, 0.15);
+  border-radius: calc(var(--radius-lg) * 0.85);
+  backdrop-filter: blur(16px);
+}
+
+.focus-ring {
+  outline: 2px solid var(--color-accent);
+  outline-offset: 4px;
+}
+
+.visually-balanced {
+  letter-spacing: -0.01em;
+  font-variation-settings: "wght" 560;
+}
+
+details > summary::-webkit-details-marker,
+details > summary::marker {
+  display: none;
+}
