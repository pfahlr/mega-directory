  name: string;
  description: string;
  listings: number;
  slug: string;
}

const { category }: { category: Category } = Astro.props;
const formattedCount = new Intl.NumberFormat('en-US').format(category.listings);
---

<article class="card-surface flex h-full flex-col gap-4 p-5">
  <div class="flex items-center gap-3">
    <span class="inline-flex h-10 w-10 items-center justify-center rounded-2xl bg-white/5 text-lg">
      {category.icon}
    </span>
    <div>
      <h3 class="text-base font-semibold text-white">{category.name}</h3>
      <p class="text-sm text-slate-400">{category.description}</p>
    </div>
  </div>
  <div class="flex items-center justify-between text-sm text-slate-400">
    <span>{formattedCount} listings</span>
    <a
      class="font-semibold text-emerald-300 transition hover:text-emerald-200 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-4 focus-visible:outline-emerald-400"
      href={`/categories/${category.slug}`}
      aria-label={`View ${category.name} listings`}
    >
      Explore
    </a>
  </div>
</article>
