+    <p class="text-lg text-slate-300 md:text-xl">
+      {description}
+    </p>
+    <div class="flex flex-wrap gap-3">
+      {[primaryAction, secondaryAction].map((action) => (
+        <a
+          href={action.href}
+          aria-label={action.ariaLabel ?? action.label}
+          class={`inline-flex items-center justify-center rounded-full px-6 py-3 text-sm font-semibold transition focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-4 focus-visible:outline-emerald-400 ${resolveVariantClass(
+            action.variant
+          )}`}
+        >
+          {action.label}
+        </a>
+      ))}
+    </div>
+    <dl class="mt-6 grid gap-6 sm:grid-cols-3">
+      {stats.map((stat) => (
+        <div class="rounded-2xl border border-white/10 bg-white/5 p-4 text-center shadow-inner shadow-black/20">
+          <dt class="text-sm uppercase tracking-[0.2em] text-slate-400">{stat.label}</dt>
+          <dd class="mt-1 text-3xl font-semibold text-white">{stat.value}</dd>
+          {stat.helper && <p class="text-xs text-slate-400">{stat.helper}</p>}
+        </div>
+      ))}
+    </dl>
+  </div>
+  <div class="glass-panel flex flex-col justify-between border border-white/10 p-6">
