+        <a
+          href={action.href}
+          aria-label={action.ariaLabel ?? action.label}
+          class={`inline-flex items-center justify-center rounded-full px-6 py-3 text-sm font-semibold transition focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-4 focus-visible:outline-emerald-400 ${resolveVariantClass(
