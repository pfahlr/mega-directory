+    'Composable filters for categories, locations, and scores.',
+    'Admin workflow with audit trails & moderation cues.',
+  ],
+} = Astro.props;
+
+const resolveVariantClass = (variant: ActionLink['variant']) =>
+  variant === 'secondary'
